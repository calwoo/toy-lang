(define factorial
    (lambda (n)
        (if (= n 0)
            1
            (* n (factorial (- n 1))))))

(define loop
    (lambda (start end f)
        (if (= start end)
            #u
            (block (print start)
                   (print (f start))
                   (loop (+ start 1) end f)))))

(print 1000)
(loop 0 10 factorial)
